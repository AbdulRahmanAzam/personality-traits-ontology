@prefix :  <http://www.personality-ontology.org/bigfive#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.personality-ontology.org/bigfive> .

# ===============================================
# ONTOLOGY DECLARATION
# ===============================================

<http://www.personality-ontology.org/bigfive> rdf:type owl:Ontology ;
    rdfs:label "Big Five Personality Traits Ontology" ;
    rdfs:comment "An ontology for the Big Five personality model based on IPIP-50 scale and meta-analytic research." ;
    owl:versionInfo "1.0.0" .

# ===============================================
# CLASSES
# ===============================================

# Main Personality Trait Classes
 :PersonalityTrait rdf:type owl:Class ;
    rdfs:label "Personality Trait" ;
    rdfs:comment "A broad dimension of personality in the Big Five model" . 

 :Extraversion rdf:type owl:Class ;
    rdfs:subClassOf :PersonalityTrait ;
    rdfs:label "Extraversion" ;
    rdfs:comment "Tendency toward sociability, assertiveness, and positive emotions" .

:Agreeableness rdf:type owl:Class ;
    rdfs:subClassOf :PersonalityTrait ;
    rdfs:label "Agreeableness" ;
    rdfs:comment "Tendency toward cooperation, trust, and empathy" .

:Conscientiousness rdf:type owl:Class ;
    rdfs:subClassOf :PersonalityTrait ;
    rdfs:label "Conscientiousness" ;
    rdfs:comment "Tendency toward organization, dependability, and self-discipline" .

:Neuroticism rdf:type owl:Class ;
    rdfs:subClassOf :PersonalityTrait ;
    rdfs:label "Neuroticism" ;
    rdfs:comment "Tendency toward negative emotions and emotional instability" .

 :Openness rdf:type owl:Class ;
    rdfs:subClassOf :PersonalityTrait ;
    rdfs:label "Openness to Experience" ;
    rdfs:comment "Tendency toward creativity, curiosity, and openness to new ideas" .

# Question Classes
:Question rdf:type owl:Class ;
    rdfs:label "Question" ;
    rdfs:comment "A question item from the IPIP-50 personality assessment" .

 :PositivelyKeyedQuestion rdf:type owl:Class ;
    rdfs:subClassOf :Question ;
    rdfs:label "Positively Keyed Question" ;
    rdfs:comment "Higher response indicates higher trait level" .

:NegativelyKeyedQuestion rdf:type owl:Class ;
    rdfs:subClassOf :Question ;
    rdfs:label "Negatively Keyed Question" ;
    rdfs:comment "Higher response indicates lower trait level (reverse scored)" .

# Response and Assessment Classes
:Response rdf:type owl:Class ;
    rdfs:label "Response" . 

:LikertOption rdf:type owl:Class ;
    rdfs:label "Likert Option" . 

:Assessment rdf:type owl:Class ;
    rdfs:label "Assessment" ;
    rdfs:comment "A personality assessment instrument" . 

# Person Class
:Person rdf:type owl:Class ;
    rdfs:label "Person" ;
    rdfs:comment "An individual who takes a personality assessment" .

# Score Classes
:TraitScore rdf:type owl:Class ;
    rdfs:label "Trait Score" .

 :ScoreCategory rdf:type owl:Class ;
    rdfs:label "Score Category" .

# Performance Outcome Classes
:PerformanceOutcome rdf:type owl:Class ;
    rdfs:label "Performance Outcome" .

:JobPerformance rdf:type owl:Class ;
    rdfs:subClassOf :PerformanceOutcome ;
    rdfs:label "Job Performance" .

:AcademicPerformance rdf:type owl:Class ;
    rdfs:subClassOf :PerformanceOutcome ;
    rdfs:label "Academic Performance" . 

:LeadershipEffectiveness rdf:type owl:Class ;
    rdfs:subClassOf :PerformanceOutcome ;
    rdfs:label "Leadership Effectiveness" .


# Correlation Finding Class
:CorrelationFinding rdf:type owl:Class ;
    rdfs:label "Correlation Finding" ;
    rdfs:comment "A statistical finding representing the correlation between a personality trait and a performance outcome, aggregated from research studies" .

:PopulationNorm rdf:type owl:Class ;
    rdfs:label "Population Norm" .

# ===============================================
# OBJECT PROPERTIES
# ===============================================

 :measuresTrait rdf:type owl:ObjectProperty ;
    rdfs:domain :Question ;
    rdfs:range :PersonalityTrait ;
    rdfs:label "measures trait" .

:belongsToPerson rdf:type owl:ObjectProperty ;
    rdfs:domain :TraitScore ;
    rdfs:range :Person ;
    rdfs:label "belongs to person" . 

:forTrait rdf:type owl:ObjectProperty ;
    rdfs:domain :TraitScore ;
    rdfs:range :PersonalityTrait ;
    rdfs:label "for trait" .

:hasTraitScore rdf:type owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :TraitScore ;
    rdfs:label "has trait score" .

:predictsOutcome rdf:type owl:ObjectProperty ;
    rdfs:domain :PersonalityTrait ;
    rdfs:range :PerformanceOutcome ;
    rdfs:label "predicts outcome" . 

:hasScoreCategory rdf:type owl:ObjectProperty ;
    rdfs:domain :TraitScore ;
    rdfs:range :ScoreCategory ;
    rdfs:label "has score category" .

:hasNorm rdf:type owl:ObjectProperty ;
    rdfs:domain :PersonalityTrait ;
    rdfs:range :PopulationNorm ;
    rdfs:label "has norm" .

:forOutcome rdf:type owl:ObjectProperty ;
    rdfs:domain :CorrelationFinding ;
    rdfs:range :PerformanceOutcome ;
    rdfs:label "for outcome" .

 :regardingTrait rdf:type owl:ObjectProperty ;
    rdfs:domain :CorrelationFinding ;
    rdfs:range :PersonalityTrait ;
    rdfs:label "regarding trait" . 

# ===============================================
# DATA PROPERTIES
# ===============================================

:questionID rdf:type owl:DatatypeProperty ;
    rdfs:domain :Question ;
    rdfs:range xsd:integer ;
    rdfs:label "question ID" .

:questionText rdf:type owl:DatatypeProperty ;
    rdfs:domain :Question ;
    rdfs:range xsd:string ;
    rdfs:label "question text" . 

:rawScore rdf:type owl:DatatypeProperty ;
    rdfs:domain :Response ;
    rdfs:range xsd:integer ;
    rdfs:label "raw score" .

:traitRawScore rdf:type owl:DatatypeProperty ;
    rdfs:domain :TraitScore ;
    rdfs:range xsd:integer ;
    rdfs:label "trait raw score" .

:traitPercentile rdf:type owl:DatatypeProperty ;
    rdfs:domain :TraitScore ;
    rdfs:range xsd:float ;
    rdfs:label "trait percentile" .

:traitTScore rdf:type owl:DatatypeProperty ;
    rdfs:domain :TraitScore ;
    rdfs:range xsd:float ;
    rdfs:label "trait T-score" .

 :populationMean rdf:type owl:DatatypeProperty ;
    rdfs:domain :PopulationNorm ;
    rdfs:range xsd:float ;
    rdfs:label "population mean" .

:populationStd rdf:type owl:DatatypeProperty ;
    rdfs:domain :PopulationNorm ;
    rdfs:range xsd:float ;
    rdfs:label "population standard deviation" . 

:sampleSize rdf:type owl:DatatypeProperty ;
    rdfs:domain :PopulationNorm ;
    rdfs:range xsd:integer ;
    rdfs:label "sample size" .

:correlationValue rdf:type owl:DatatypeProperty ;
    rdfs:domain :CorrelationFinding ;
    rdfs:range xsd:float ;
    rdfs:label "correlation value" .

:confidenceIntervalLower rdf:type owl:DatatypeProperty ;
    rdfs:domain :CorrelationFinding ;
    rdfs:range xsd:float ;
    rdfs:label "CI lower bound" .

:confidenceIntervalUpper rdf:type owl:DatatypeProperty ;
    rdfs:domain :CorrelationFinding ;
    rdfs:range xsd:float ;
    rdfs:label "CI upper bound" .

:numberOfStudies rdf:type owl:DatatypeProperty ;
    rdfs:domain :CorrelationFinding ;
    rdfs:range xsd:integer ;
    rdfs:label "number of studies" .

:likertValue rdf:type owl:DatatypeProperty ;
    rdfs:domain :LikertOption ;
    rdfs:range xsd:integer ;
    rdfs:label "likert value" .

:likertLabel rdf:type owl:DatatypeProperty ;
    rdfs:domain :LikertOption ;
    rdfs:range xsd:string ;
    rdfs:label "likert label" .

 :categoryName rdf:type owl:DatatypeProperty ;
    rdfs:domain :ScoreCategory ;
    rdfs:range xsd:string ;
    rdfs:label "category name" .

:categoryMinPercentile rdf:type owl:DatatypeProperty ;
    rdfs:domain :ScoreCategory ;
    rdfs:range xsd:float ;
    rdfs:label "min percentile" .

:categoryMaxPercentile rdf:type owl:DatatypeProperty ;
    rdfs:domain :ScoreCategory ;
    rdfs:range xsd:float ;
    rdfs:label "max percentile" . 

# ===============================================
# INDIVIDUALS - LIKERT OPTIONS
# ===============================================

 :Likert1 rdf:type :LikertOption ;
