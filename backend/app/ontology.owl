    :likertLabel "Very Accurate" . 

# ===============================================
# INDIVIDUALS - SCORE CATEGORIES
# ===============================================

:VeryLow rdf:type :ScoreCategory ;
    :categoryName "Very Low" ;
    :categoryMinPercentile 0.0 ;
    :categoryMaxPercentile 20.0 .

:Low rdf:type :ScoreCategory ;
    :categoryName "Low" ;
    :categoryMinPercentile 20.0 ;
    :categoryMaxPercentile 40.0 .

 :Average rdf:type :ScoreCategory ;
    :categoryName "Average" ;
    :categoryMinPercentile 40.0 ;
    :categoryMaxPercentile 60.0 .

 :High rdf:type :ScoreCategory ;
    :categoryName "High" ;
    :categoryMinPercentile 60.0 ;
    :categoryMaxPercentile 80.0 . 

:VeryHigh rdf:type :ScoreCategory ;
    :categoryName "Very High" ;
    :categoryMinPercentile 80.0 ;
    :categoryMaxPercentile 100.0 .

# ===============================================
# INDIVIDUALS - TRAIT INSTANCES WITH NORMS
# ===============================================

 :ExtraversionTrait rdf:type :Extraversion ;
    rdfs:label "Extraversion" ;
 :hasNorm :ExtraversionNorm ;
    :predictsOutcome :JobPerformanceOutcome ;
    :predictsOutcome :LeadershipEffectivenessOutcome .

 :ExtraversionNorm rdf:type :PopulationNorm ;
    :populationMean 29.6 ;
    :populationStd 6.2 ;
    :sampleSize 23994 .

:AgreeablenessTrait rdf:type :Agreeableness ;
    rdfs:label "Agreeableness" ;
    :hasNorm :AgreeablenessNorm .

:AgreeablenessNorm rdf:type :PopulationNorm ;
    :populationMean 36.0 ;
    :populationStd 5.4 ;
    :sampleSize 23994 .

:ConscientiousnessTrait rdf:type :Conscientiousness ;
    rdfs:label "Conscientiousness" ;
    :hasNorm :ConscientiousnessNorm ;
    :predictsOutcome :JobPerformanceOutcome ;
    :predictsOutcome :AcademicPerformanceOutcome ;
    :predictsOutcome :LeadershipEffectivenessOutcome .

:ConscientiousnessNorm rdf:type :PopulationNorm ;
    :populationMean 32.3 ;
    :populationStd 5.8 ;
    :sampleSize 23994 .

 :NeuroticismTrait rdf:type :Neuroticism ;
    rdfs:label "Neuroticism" ;
 :hasNorm :NeuroticismNorm ;
     :predictsOutcome :JobPerformanceOutcome .

 :NeuroticismNorm rdf:type :PopulationNorm ;
    :populationMean 30.6 ;
    :populationStd 7.1 ;
    :sampleSize 23994 .

:OpennessTrait rdf:type :Openness ;
    rdfs:label "Openness to Experience" ;
    :hasNorm :OpennessNorm ;
 :predictsOutcome :AcademicPerformanceOutcome ;
    :predictsOutcome :LeadershipEffectivenessOutcome .

:OpennessNorm rdf:type :PopulationNorm ;
 :populationMean 38.7 ;
    :populationStd 5.9 ;
    :sampleSize 23994 .

# ===============================================
# INDIVIDUALS - PERFORMANCE OUTCOMES
# ===============================================

:JobPerformanceOutcome rdf:type :JobPerformance ;
    rdfs:label "Job Performance" . 

:AcademicPerformanceOutcome rdf:type :AcademicPerformance ;
    rdfs:label "Academic Performance" .

:LeadershipEffectivenessOutcome rdf:type :LeadershipEffectiveness ;
    rdfs:label "Leadership Effectiveness" .

# ===============================================
# INDIVIDUALS - CORRELATION FINDINGS
# ===============================================

# Conscientiousness → Job Performance
:C_JobPerf_Correlation rdf:type :CorrelationFinding ;
    :regardingTrait :ConscientiousnessTrait ;
    :forOutcome :JobPerformanceOutcome ;
    :correlationValue 0.27 ;
    :confidenceIntervalLower 0.22 ;
    :confidenceIntervalUpper 0.32 ;
    :numberOfStudies 239 .

# Extraversion → Job Performance
:E_JobPerf_Correlation rdf:type :CorrelationFinding ;
 :regardingTrait :ExtraversionTrait ;
 :forOutcome :JobPerformanceOutcome ;
    :correlationValue 0.13 ;
    :confidenceIntervalLower 0.08 ;
    :confidenceIntervalUpper 0.18 ;
    :numberOfStudies 182 .

# Neuroticism → Job Performance (Negative)
:N_JobPerf_Correlation rdf:type :CorrelationFinding ;
    :regardingTrait :NeuroticismTrait ;
    :forOutcome :JobPerformanceOutcome ;
    :correlationValue -0.13 ;
    :confidenceIntervalLower -0.18 ;
 :confidenceIntervalUpper -0.08 ;
    :numberOfStudies 165 .

# Agreeableness → Job Performance
 :A_JobPerf_Correlation rdf:type :CorrelationFinding ;
    :regardingTrait :AgreeablenessTrait ;
    :forOutcome :JobPerformanceOutcome ;
    :correlationValue 0.10 ;
    :confidenceIntervalLower 0.05 ;
    :confidenceIntervalUpper 0.15 ;
    :numberOfStudies 142 .

# Openness → Job Performance
:O_JobPerf_Correlation rdf:type :CorrelationFinding ;
    :regardingTrait :OpennessTrait ;
    :forOutcome :JobPerformanceOutcome ;
    :correlationValue 0.07 ;
    :confidenceIntervalLower 0.02 ;
    :confidenceIntervalUpper 0.12 ;
    :numberOfStudies 128 .

# Conscientiousness → Academic Performance
:C_Academic_Correlation rdf:type :CorrelationFinding ;
 :regardingTrait :ConscientiousnessTrait ;
 :forOutcome :AcademicPerformanceOutcome ;
